    private void alargenumberActionPerformed(java.awt.event.ActionEvent evt) {                                             
        // TODO add your handling code here:
    }                                            

    private void acalculateActionPerformed(java.awt.event.ActionEvent evt) {                                           
        // TODO add your handling code here:
        double num1,num2, result,result1, result2,result3, pst, gst;
        num2=Double.parseDouble(alargenumber.getText());
        num1=Double.parseDouble(axlnumber.getText());
        result1=num1 * 10;
        result2=num2 * 6;
        pst=(result1+result2)*0.08;
        gst=(result1+result2)*0.06;
        result=result1+result2;
        result3 = result1+result2+gst+pst;
        apst.setText(String.valueOf(pst));
        agst.setText(String.valueOf(gst));
        aresult2.setText(String.valueOf(result));
        aresult.setText(String.valueOf(result3));
    }                                          

    private void aquitActionPerformed(java.awt.event.ActionEvent evt) {                                      
        // TODO add your handling code here:
        System.exit(0);
    }                                     

    private void aneworderActionPerformed(java.awt.event.ActionEvent evt) {                                          
        // TODO add your handling code here:
        axlnumber.setText("");
        alargenumber.setText("");
        aresult.setText("");
        agst.setText("");
        apst.setText("");
        aresult2.setText("");
    }                                         

    private void jComboBox1ActionPerformed(java.awt.event.ActionEvent evt) {                                           
        // TODO add your handling code here:
         String itemSelected = jComboBox1.getSelectedItem() + "";
         double num1,num2, result,result1, result2,result3, pst, gst;
        num2=Double.parseDouble(alargenumber.getText());
        num1=Double.parseDouble(axlnumber.getText());
        
        switch (itemSelected) {
            case "0" -> {
                result1=num1 * 10;
                result2=num2 * 6;
                result=result1 + result2;
                pst=(result1+result2)*0.08;
                gst=(result1+result2)*0.06;
                result3 = result1+result2+gst+pst;
                apst.setText(String.valueOf(pst));
                agst.setText(String.valueOf(gst));
                aresult2.setText(String.valueOf(result));
                aresult.setText(String.valueOf(result3));
            }
            case "1" -> {
                result1=num1 * 10;
                result2=num2 * 6;
                result=result1 + result2;
                pst=(result1+result2+1.00)*0.08;
                gst=(result1+result2+1.00)*0.06;
                result3 = result1+result2+gst+pst;
                apst.setText(String.valueOf(pst));
                agst.setText(String.valueOf(gst));
                aresult2.setText(String.valueOf(result));
                aresult.setText(String.valueOf(result3));
            }
            case "2" -> {
                result1=num1 * 10;
                result2=num2 * 6;
                result=result1 + result2;
                pst=(result1+result2+1.75)*0.08;
                gst=(result1+result2+1.75)*0.06;
                result3 = result1+result2+gst+pst;
                apst.setText(String.valueOf(pst));
                agst.setText(String.valueOf(gst));
                aresult2.setText(String.valueOf(result));
                aresult.setText(String.valueOf(result3));
            }
            case "3" -> {
                result1=num1 * 10;
                result2=num2 * 6;
                result=result1 + result2;
                pst=(result1+result2+2.50)*0.08;
                gst=(result1+result2+2.50)*0.06;
                result3 = result1+result2+gst+pst;
                apst.setText(String.valueOf(pst));
                agst.setText(String.valueOf(gst));
                aresult2.setText(String.valueOf(result));
                aresult.setText(String.valueOf(result3));
            }
            case "4" -> {
                result1=num1 * 10;
                result2=num2 * 6;
                result=result1 + result2;
                pst=(result1+result2+3.25)*0.08;
                gst=(result1+result2+3.25)*0.06;
                result3 = result1+result2+gst+pst;
                apst.setText(String.valueOf(pst));
                agst.setText(String.valueOf(gst));
                aresult2.setText(String.valueOf(result));
                aresult.setText(String.valueOf(result3));
            }
        }
    }                                          

